{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "libs": {
      "root": "libs",
      "sourceRoot": "libs",
      "projectType": "library",
      "prefix": "foo",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.libs.ts",
            "tsConfig": "testing/tsconfig.libs.spec.json",
            "karmaConfig": "testing/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.libs.json",
              "testing/tsconfig.libs.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "xplat": {
      "root": "xplat",
      "sourceRoot": "xplat",
      "projectType": "library",
      "prefix": "foo",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "testing/test.xplat.ts",
            "tsConfig": "testing/tsconfig.xplat.spec.json",
            "karmaConfig": "testing/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.xplat.json",
              "testing/tsconfig.xplat.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ionic-mobile": {
      "root": "apps/ionic-mobile/",
      "sourceRoot": "apps/ionic-mobile/src",
      "projectType": "application",
      "prefix": "foo",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "budget-tool": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/budget-tool",
      "sourceRoot": "apps/budget-tool/src",
      "prefix": "budget",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/budget-tool",
            "index": "apps/budget-tool/src/index.html",
            "main": "apps/budget-tool/src/main.ts",
            "polyfills": "apps/budget-tool/src/polyfills.ts",
            "tsConfig": "apps/budget-tool/tsconfig.app.json",
            "assets": [
              "apps/budget-tool/src/favicon.ico",
              "apps/budget-tool/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared-assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/budget-tool/src/styles.scss",
              "libs/scss/_index.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/budget-tool/src/environments/environment.ts",
                  "with": "apps/budget-tool/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "budget-tool:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "budget-tool:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "budget-tool:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/budget-tool/tsconfig.app.json",
              "apps/budget-tool/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/budget-tool/jest.config.js",
            "tsConfig": "apps/budget-tool/tsconfig.spec.json",
            "setupFile": "apps/budget-tool/src/test-setup.ts"
          }
        }
      }
    },
    "budget-tool-e2e": {
      "root": "apps/budget-tool-e2e",
      "sourceRoot": "apps/budget-tool-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/budget-tool-e2e/cypress.json",
            "tsConfig": "apps/budget-tool-e2e/tsconfig.e2e.json",
            "devServerTarget": "budget-tool:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "budget-tool:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/budget-tool-e2e/tsconfig.e2e.json"
          }
        }
      }
    },
